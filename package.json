        "alpinejs": "^3.13.3",
        "axios": "^1.6.2",
        "chart.js": "^4.4.0",
        "dayjs": "^1.11.10",
        "laravel-vite-plugin": "^0.8.1",
        "pinia": "^2.1.7",
        "pusher-js": "^8.4.0-rc2",
        "quill": "^1.3.7",
        "vue": "^3.3.8",
